{
  "name": "kotlin2js",
  "version": "1.0.0",
  "description": "Generating a node app from Kotlin source",
  "main": "build/js/module.js",
  "scripts": {
    "start": "node build/js/module.js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/phatblat/Kotlin2NodeJS.git"
  },
  "keywords": [
    "kotlin",
    "node"
  ],
  "author": "Ben Chatelain",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/phatblat/Kotlin2NodeJS/issues"
  },
  "homepage": "https://github.com/phatblat/Kotlin2NodeJS#readme",
  "dependencies": {
    "kotlin": "^1.1.4",
    "realm": "^1.10.3"
  },
  "eslintConfig": {
    "env": {
      "es6": true,
      "node": true
    },
    "parserOptions": {
      "ecmaVersion": 6,
      "sourceType": "module"
    },
    "ecmaFeatures": {
      "impliedStrict": true
    },
    "rules": {
      "semi": "off"
    }
  },
  "devDependencies": {
    "ts2kt": "0.0.14"
  }
}
